PedidosPizzaria

CREATE TABLE pedidos(
ID INT PRIMARY KEY auto_increment,
data_pedido Date,
sabor INT,
tamanho VARCHAR(255),
quantidade INT,
nome_completo VARCHA(255)
CPF BIGINT
);


